            } else {
                $('gan-block-content-content').addClassName("gan-hidden");
                $('gan-block-content-content').hide();
                $(control).innerHTML = 'Show';
                this.navigationOpenFilters['gan_bcontent_hide'] = true;
            }
        }
    }
});

